  "language": "Hebrew",
  "verses": [
    {
      "book_id": "PRO",
      "book_name": "Proverbs",
      "chapter": 9,
      "verse_start": 1,
      "verse_end": 1,
      "text": "חכמות בנתה ביתה חצבה עמודיה שׁבעה׃",
      "info": ""
    },
    {
      "book_id": "PRO",
      "book_name": "Proverbs",
      "chapter": 9,
      "verse_start": 2,
      "verse_end": 2,
      "text": "טבחה טבחה מסכה יינה אף ערכה שׁלחנה׃",
      "info": ""
    },
    {
      "book_id": "PRO",
      "book_name": "Proverbs",
      "chapter": 9,
      "verse_start": 3,
      "verse_end": 3,
      "text": "שׁלחה נערתיה תקרא על גפי מרמי קרת׃",
      "info": ""
    },
    {
      "book_id": "PRO",
      "book_name": "Proverbs",
      "chapter": 9,
      "verse_start": 4,
      "verse_end": 4,
      "text": "מי פתי יסר הנה חסר לב אמרה׃",
      "info": ""
    },
    {
      "book_id": "PRO",
      "book_name": "Proverbs",
      "chapter": 9,
      "verse_start": 5,
      "verse_end": 5,
      "text": "לכו לחמו בלחמי ושׁתו ביין מסכתי׃",
      "info": ""
    },
    {
      "book_id": "PRO",
      "book_name": "Proverbs",
      "chapter": 9,
      "verse_start": 6,
      "verse_end": 6,
      "text": "עזבו פתאים וחיו ואשׁרו בדרך בינה׃",
      "info": ""
    },
    {
      "book_id": "PRO",
      "book_name": "Proverbs",
      "chapter": 9,
      "verse_start": 7,
      "verse_end": 7,
      "text": "יסר לץ לקח לו קלון ומוכיח לרשׁע מומו׃",
      "info": ""
    },
    {
      "book_id": "PRO",
      "book_name": "Proverbs",
      "chapter": 9,
      "verse_start": 8,
      "verse_end": 8,
      "text": "אל תוכח לץ פן ישׂנאך הוכח לחכם ויאהבך׃",
      "info": ""
    },
    {
      "book_id": "PRO",
      "book_name": "Proverbs",
      "chapter": 9,
      "verse_start": 9,
      "verse_end": 9,
      "text": "תן לחכם ויחכם עוד הודע לצדיק ויוסף לקח׃",
      "info": ""
    },
    {
      "book_id": "PRO",
      "book_name": "Proverbs",
      "chapter": 9,
      "verse_start": 10,
      "verse_end": 10,
      "text": "תחלת חכמה יראת יהוה ודעת קדשׁים בינה׃",
      "info": ""
    },
    {
      "book_id": "PRO",
      "book_name": "Proverbs",
      "chapter": 9,
      "verse_start": 11,
      "verse_end": 11,
      "text": "כי בי ירבו ימיך ויוסיפו לך שׁנות חיים׃",
      "info": ""
    },
    {
      "book_id": "PRO",
      "book_name": "Proverbs",
      "chapter": 9,
      "verse_start": 12,
      "verse_end": 12,
      "text": "אם חכמת חכמת לך ולצת לבדך תשׂא׃",
      "info": ""
    },
    {
      "book_id": "PRO",
      "book_name": "Proverbs",
      "chapter": 9,
      "verse_start": 13,
      "verse_end": 13,
      "text": "אשׁת כסילות המיה פתיות ובל ידעה מה׃",
      "info": ""
    },
    {
      "book_id": "PRO",
      "book_name": "Proverbs",
      "chapter": 9,
      "verse_start": 14,
      "verse_end": 14,
      "text": "וישׁבה לפתח ביתה על כסא מרמי קרת׃",
      "info": ""
    },
    {
      "book_id": "PRO",
      "book_name": "Proverbs",
      "chapter": 9,
      "verse_start": 15,
      "verse_end": 15,
      "text": "לקרא לעברי דרך המישׁרים ארחותם׃",
      "info": ""
    },
    {
      "book_id": "PRO",
      "book_name": "Proverbs",
      "chapter": 9,
      "verse_start": 16,
      "verse_end": 16,
      "text": "מי פתי יסר הנה וחסר לב ואמרה׃",
      "info": ""
    },
    {
      "book_id": "PRO",
      "book_name": "Proverbs",
      "chapter": 9,
      "verse_start": 17,
      "verse_end": 17,
      "text": "מים גנובים ימתקו ולחם סתרים ינעם׃",
      "info": ""
    },
    {
      "book_id": "PRO",
      "book_name": "Proverbs",
      "chapter": 9,
      "verse_start": 18,
      "verse_end": 18,
      "text": "ולא ידע כי רפאים שׁם בעמקי שׁאול קראיה׃",
      "info": ""
    }
  ]
}