  "language": "Hebrew",
  "verses": [
    {
      "book_id": "JOB",
      "book_name": "Job",
      "chapter": 34,
      "verse_start": 1,
      "verse_end": 1,
      "text": "ויען אליהוא ויאמר׃",
      "info": ""
    },
    {
      "book_id": "JOB",
      "book_name": "Job",
      "chapter": 34,
      "verse_start": 2,
      "verse_end": 2,
      "text": "שׁמעו חכמים מלי וידעים האזינו׃",
      "info": ""
    },
    {
      "book_id": "JOB",
      "book_name": "Job",
      "chapter": 34,
      "verse_start": 3,
      "verse_end": 3,
      "text": "כי אזן מלין תבחן וחך יטעם לאכל׃",
      "info": ""
    },
    {
      "book_id": "JOB",
      "book_name": "Job",
      "chapter": 34,
      "verse_start": 4,
      "verse_end": 4,
      "text": "משׁפט נבחרה לנו נדעה בינינו מה טוב׃",
      "info": ""
    },
    {
      "book_id": "JOB",
      "book_name": "Job",
      "chapter": 34,
      "verse_start": 5,
      "verse_end": 5,
      "text": "כי אמר איוב צדקתי ואל הסיר משׁפטי׃",
      "info": ""
    },
    {
      "book_id": "JOB",
      "book_name": "Job",
      "chapter": 34,
      "verse_start": 6,
      "verse_end": 6,
      "text": "על משׁפטי אכזב אנושׁ חצי בלי פשׁע׃",
      "info": ""
    },
    {
      "book_id": "JOB",
      "book_name": "Job",
      "chapter": 34,
      "verse_start": 7,
      "verse_end": 7,
      "text": "מי גבר כאיוב ישׁתה לעג כמים׃",
      "info": ""
    },
    {
      "book_id": "JOB",
      "book_name": "Job",
      "chapter": 34,
      "verse_start": 8,
      "verse_end": 8,
      "text": "וארח לחברה עם פעלי און וללכת עם אנשׁי רשׁע׃",
      "info": ""
    },
    {
      "book_id": "JOB",
      "book_name": "Job",
      "chapter": 34,
      "verse_start": 9,
      "verse_end": 9,
      "text": "כי אמר לא יסכן גבר ברצתו עם אלהים׃",
      "info": ""
    },
    {
      "book_id": "JOB",
      "book_name": "Job",
      "chapter": 34,
      "verse_start": 10,
      "verse_end": 10,
      "text": "לכן אנשׁי לבב שׁמעו לי חללה לאל מרשׁע ושׁדי מעול׃",
      "info": ""
    },
    {
      "book_id": "JOB",
      "book_name": "Job",
      "chapter": 34,
      "verse_start": 11,
      "verse_end": 11,
      "text": "כי פעל אדם ישׁלם לו וכארח אישׁ ימצאנו׃",
      "info": ""
    },
    {
      "book_id": "JOB",
      "book_name": "Job",
      "chapter": 34,
      "verse_start": 12,
      "verse_end": 12,
      "text": "אף אמנם אל לא ירשׁיע ושׁדי לא יעות משׁפט׃",
      "info": ""
    },
    {
      "book_id": "JOB",
      "book_name": "Job",
      "chapter": 34,
      "verse_start": 13,
      "verse_end": 13,
      "text": "מי פקד עליו ארצה ומי שׂם תבל כלה׃",
      "info": ""
    },
    {
      "book_id": "JOB",
      "book_name": "Job",
      "chapter": 34,
      "verse_start": 14,
      "verse_end": 14,
      "text": "אם ישׂים אליו לבו רוחו ונשׁמתו אליו יאסף׃",
      "info": ""
    },
    {
      "book_id": "JOB",
      "book_name": "Job",
      "chapter": 34,
      "verse_start": 15,
      "verse_end": 15,
      "text": "יגוע כל בשׂר יחד ואדם על עפר ישׁוב׃",
      "info": ""
    },
    {
      "book_id": "JOB",
      "book_name": "Job",
      "chapter": 34,
      "verse_start": 16,
      "verse_end": 16,
      "text": "ואם בינה שׁמעה זאת האזינה לקול מלי׃",
      "info": ""
    },
    {
      "book_id": "JOB",
      "book_name": "Job",
      "chapter": 34,
      "verse_start": 17,
      "verse_end": 17,
      "text": "האף שׂונא משׁפט יחבושׁ ואם צדיק כביר תרשׁיע׃",
      "info": ""
    },
    {
      "book_id": "JOB",
      "book_name": "Job",
      "chapter": 34,
      "verse_start": 18,
      "verse_end": 18,
      "text": "האמר למלך בליעל רשׁע אל נדיבים׃",
      "info": ""
    },
    {
      "book_id": "JOB",
      "book_name": "Job",
      "chapter": 34,
      "verse_start": 19,
      "verse_end": 19,
      "text": "אשׁר לא נשׂא פני שׂרים ולא נכר שׁוע לפני דל כי מעשׂה ידיו כלם׃",
      "info": ""
    },
    {
      "book_id": "JOB",
      "book_name": "Job",
      "chapter": 34,
      "verse_start": 20,
      "verse_end": 20,
      "text": "רגע ימתו וחצות לילה יגעשׁו עם ויעברו ויסירו אביר לא ביד׃",
      "info": ""
    },
    {
      "book_id": "JOB",
      "book_name": "Job",
      "chapter": 34,
      "verse_start": 21,
      "verse_end": 21,
      "text": "כי עיניו על דרכי אישׁ וכל צעדיו יראה׃",
      "info": ""
    },
    {
      "book_id": "JOB",
      "book_name": "Job",
      "chapter": 34,
      "verse_start": 22,
      "verse_end": 22,
      "text": "אין חשׁך ואין צלמות להסתר שׁם פעלי און׃",
      "info": ""
    },
    {
      "book_id": "JOB",
      "book_name": "Job",
      "chapter": 34,
      "verse_start": 23,
      "verse_end": 23,
      "text": "כי לא על אישׁ ישׂים עוד להלך אל אל במשׁפט׃",
      "info": ""
    },
    {
      "book_id": "JOB",
      "book_name": "Job",
      "chapter": 34,
      "verse_start": 24,
      "verse_end": 24,
      "text": "ירע כבירים לא חקר ויעמד אחרים תחתם׃",
      "info": ""
    },
    {
      "book_id": "JOB",
      "book_name": "Job",
      "chapter": 34,
      "verse_start": 25,
      "verse_end": 25,
      "text": "לכן יכיר מעבדיהם והפך לילה וידכאו׃",
      "info": ""
    },
    {
      "book_id": "JOB",
      "book_name": "Job",
      "chapter": 34,
      "verse_start": 26,
      "verse_end": 26,
      "text": "תחת רשׁעים ספקם במקום ראים׃",
      "info": ""
    },
    {
      "book_id": "JOB",
      "book_name": "Job",
      "chapter": 34,
      "verse_start": 27,
      "verse_end": 27,
      "text": "אשׁר על כן סרו מאחריו וכל דרכיו לא השׂכילו׃",
      "info": ""
    },
    {
      "book_id": "JOB",
      "book_name": "Job",
      "chapter": 34,
      "verse_start": 28,
      "verse_end": 28,
      "text": "להביא עליו צעקת דל וצעקת עניים ישׁמע׃",
      "info": ""
    },
    {
      "book_id": "JOB",
      "book_name": "Job",
      "chapter": 34,
      "verse_start": 29,
      "verse_end": 29,
      "text": "והוא ישׁקט ומי ירשׁע ויסתר פנים ומי ישׁורנו ועל גוי ועל אדם יחד׃",
      "info": ""
    },
    {
      "book_id": "JOB",
      "book_name": "Job",
      "chapter": 34,
      "verse_start": 30,
      "verse_end": 30,
      "text": "ממלך אדם חנף ממקשׁי עם׃",
      "info": ""
    },
    {
      "book_id": "JOB",
      "book_name": "Job",
      "chapter": 34,
      "verse_start": 31,
      "verse_end": 31,
      "text": "כי אל אל האמר נשׂאתי לא אחבל׃",
      "info": ""
    },
    {
      "book_id": "JOB",
      "book_name": "Job",
      "chapter": 34,
      "verse_start": 32,
      "verse_end": 32,
      "text": "בלעדי אחזה אתה הרני אם עול פעלתי לא אסיף׃",
      "info": ""
    },
    {
      "book_id": "JOB",
      "book_name": "Job",
      "chapter": 34,
      "verse_start": 33,
      "verse_end": 33,
      "text": "המעמך ישׁלמנה כי מאסת כי אתה תבחר ולא אני ומה ידעת דבר׃",
      "info": ""
    },
    {
      "book_id": "JOB",
      "book_name": "Job",
      "chapter": 34,
      "verse_start": 34,
      "verse_end": 34,
      "text": "אנשׁי לבב יאמרו לי וגבר חכם שׁמע׃",
      "info": ""
    },
    {
      "book_id": "JOB",
      "book_name": "Job",
      "chapter": 34,
      "verse_start": 35,
      "verse_end": 35,
      "text": "איוב לא בדעת ידבר ודבריו לא בהשׂכיל׃",
      "info": ""
    },
    {
      "book_id": "JOB",
      "book_name": "Job",
      "chapter": 34,
      "verse_start": 36,
      "verse_end": 36,
      "text": "אבי יבחן איוב עד נצח על תשׁבת באנשׁי און׃",
      "info": ""
    },
    {
      "book_id": "JOB",
      "book_name": "Job",
      "chapter": 34,
      "verse_start": 37,
      "verse_end": 37,
      "text": "כי יסיף על חטאתו פשׁע בינינו יספוק וירב אמריו לאל׃",
      "info": ""
    }
  ]
}