        "no",
        "temáis;",
        "he",
        "aquí",
        "que",
        "vuestro",
        "Dios",
        "viene",
        "con",
        "venganza,",
        "con",
        "retribución;",
        "Dios",
        "mismo,",
        "Él",
        "vendrá",
        "y",
        "os",
        "salvará."
      ],
      "info": ""
    },
    {
