      "verse_start": 7,
      "verse_end": 7,
      "text": [
        "El",
        "lugar",
        "seco",
        "será",
        "tornado",
        "en",
        "estanque,",
        "y",
        "el",
        "sequedal",
        "en",
        "manaderos",
        "de",
        "aguas;",
        "en",
        "la",
        "habitación",
        "de",
        "dragones,",
        "en",
        "su",
        "guarida,",
        "<em>será</em>",
        "lugar",
        "de",
        "cañas",
        "y",
        "de",
        "juncos."
      ],
      "info": ""
    },
    {
