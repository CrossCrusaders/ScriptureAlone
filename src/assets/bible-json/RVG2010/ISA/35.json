        "habrá",
        "allí",
        "calzada",
        "y",
        "camino,",
        "y",
        "será",
        "llamado",
        "Camino",
        "de",
        "Santidad;",
        "no",
        "pasará",
        "inmundo",
        "por",
        "él,",
        "sino",
        "que",
        "<em>será</em>",
        "para",
        "ellos;",
        "los",
        "errantes,",
        "aunque",
        "fueren",
        "torpes,",
        "no",
        "se",
        "extraviarán."
      ],
      "info": ""
    },
    {
