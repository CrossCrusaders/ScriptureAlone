        "de",
        "Israel",
        "estaban",
        "reunidos",
        "en",
        "Mizpa,",
        "subieron",
        "los",
        "príncipes",
        "de",
        "los",
        "filisteos",
        "contra",
        "Israel;",
        "y",
        "cuando",
        "los",
        "hijos",
        "de",
        "Israel",
        "<em>lo</em>",
        "oyeron,",
        "tuvieron",
        "temor",
        "de",
        "los",
        "filisteos."
      ],
      "info": ""
    },
    {
      "book_id": "1SA",
      "book_name": "1Samuel",
      "chapter": 7,
      "verse_start": 8,
      "verse_end": 8,
      "text": [
