      "book_name": "1Samuel",
      "chapter": 7,
      "verse_start": 5,
      "verse_end": 5,
      "text": [
        "Y",
        "Samuel",
        "dijo:",
        "Reunid",
        "a",
        "todo",
        "Israel",
        "en",
        "Mizpa,",
        "y",
        "yo",
        "oraré",
        "por",
        "vosotros",
        "a",
        "Jehová."
