      "info": ""
    },
    {
      "book_id": "1SA",
      "book_name": "1Samuel",
      "chapter": 7,
      "verse_start": 6,
      "verse_end": 6,
      "text": [
        "Y",
        "se",
        "reunieron",
        "en",
        "Mizpa,",
        "y",
        "sacaron",
        "agua,",
        "y",
        "<em>la</em>",
        "derramaron",
        "delante",
        "de",
        "Jehová,",
        "y",
        "ayunaron",
        "aquel",
        "día,",
        "y",
        "dijeron",
        "allí:",
        "Contra",
        "Jehová",
        "hemos",
        "pecado.",
        "Y",
        "juzgó",
        "Samuel",
        "a",
        "los",
        "hijos",
        "de",
        "Israel",
        "en",
        "Mizpa."
      ],
      "info": ""
    },
    {
      "book_id": "1SA",
      "book_name": "1Samuel",
      "chapter": 7,
      "verse_start": 7,
      "verse_end": 7,
      "text": [
