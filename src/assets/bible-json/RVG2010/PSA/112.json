      "text": [
        "Bienaventurado",
        "el",
        "hombre",
        "que",
        "teme",
        "a",
        "Jehová,",
        "y",
        "en",
        "sus",
        "mandamientos",
        "se",
        "deleita",
        "en",
        "gran",
        "manera."
      ],
      "info": ""
    },
    {
      "book_id": "PSA",
      "book_name": "Salmos",
      "chapter": 112,
      "verse_start": 2,
      "verse_end": 2,
      "text": [
        "Su",
        "simiente",
        "será",
        "poderosa",
        "en",
        "la",
        "tierra;",
        "la",
        "generación",
        "de",
        "los",
        "rectos",
        "será",
        "bendita."
      ],
      "info": ""
    },
    {
      "book_id": "PSA",
      "book_name": "Salmos",
      "chapter": 112,
      "verse_start": 3,
      "verse_end": 3,
      "text": [
        "Bienes",
        "y",
        "riquezas",
        "hay",
        "en",
        "su",
        "casa;",
        "y",
        "su",
        "justicia",
        "permanece",
        "para",
        "siempre."
      ],
      "info": ""
    },
    {
      "book_id": "PSA",
      "book_name": "Salmos",
      "chapter": 112,
      "verse_start": 4,
      "verse_end": 4,
      "text": [
        "En",
        "las",
        "tinieblas",
        "resplandece",
        "luz",
        "a",
        "los",
        "rectos:",
        "Él",
        "<em>es</em>",
        "clemente,",
        "misericordioso",
        "y",
        "justo."
      ],
      "info": ""
    },
    {
      "book_id": "PSA",
      "book_name": "Salmos",
      "chapter": 112,
      "verse_start": 5,
      "verse_end": 5,
      "text": [
        "El",
        "hombre",
        "de",
        "bien",
        "tiene",
        "misericordia",
        "y",
        "presta;",
        "conduce",
        "sus",
        "asuntos",
        "con",
        "juicio."
      ],
      "info": ""
    },
    {
      "book_id": "PSA",
      "book_name": "Salmos",
      "chapter": 112,
      "verse_start": 6,
      "verse_end": 6,
      "text": [
        "Por",
        "lo",
        "cual",
        "no",
        "resbalará",
        "para",
        "siempre;",
        "en",
        "memoria",
        "eterna",
        "será",
        "el",
        "justo."
      ],
      "info": ""
    },
    {
      "book_id": "PSA",
      "book_name": "Salmos",
      "chapter": 112,
      "verse_start": 7,
      "verse_end": 7,
      "text": [
        "No",
        "tendrá",
        "temor",
        "de",
        "malas",
        "noticias;",
        "su",
        "corazón",
        "está",
        "firme,",
        "confiado",
        "en",
        "Jehová."
      ],
      "info": ""
    },
    {
      "book_id": "PSA",
      "book_name": "Salmos",
      "chapter": 112,
      "verse_start": 8,
      "verse_end": 8,
      "text": [
        "Afianzado",
        "<em>está</em>",
        "su",
        "corazón,",
        "no",
        "temerá,",
        "hasta",
        "que",
        "vea",
        "en",
        "sus",
        "enemigos",
        "<em>su</em>",
        "<em>deseo</em>."
      ],
      "info": ""
    },
    {
      "book_id": "PSA",
      "book_name": "Salmos",
      "chapter": 112,
      "verse_start": 9,
      "verse_end": 9,
      "text": [
        "Esparció,",
        "dio",
        "a",
        "los",
        "pobres;",
        "su",
        "justicia",
        "permanece",
        "para",
        "siempre;",
        "su",
        "cuerno",
        "será",
        "ensalzado",
        "en",
        "gloria."
      ],
      "info": ""
    },
    {
      "book_id": "PSA",
      "book_name": "Salmos",
      "chapter": 112,
      "verse_start": 10,
      "verse_end": 10,
      "text": [
        "Lo",
        "verá",
        "el",
        "impío,",
        "y",
        "se",
        "irritará;",
        "crujirá",
        "los",
        "dientes,",
        "y",
        "se",
        "consumirá;",
        "el",
        "deseo",
        "de",
        "los",
        "impíos",
        "perecerá."
      ],
      "info": ""
    }
