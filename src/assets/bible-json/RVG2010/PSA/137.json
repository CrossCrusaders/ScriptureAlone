      "text": [
        "Junto",
        "a",
        "los",
        "ríos",
        "de",
        "Babilonia,",
        "allí",
        "nos",
        "sentábamos,",
        "y",
        "aun",
        "llorábamos,",
        "acordándonos",
        "de",
        "Sión."
      ],
      "info": ""
    },
    {
      "book_id": "PSA",
      "book_name": "Salmos",
      "chapter": 137,
      "verse_start": 2,
      "verse_end": 2,
      "text": [
        "Sobre",
        "los",
        "sauces",
        "en",
        "medio",
        "de",
        "ella",
        "colgamos",
        "nuestras",
        "arpas."
      ],
      "info": ""
    },
    {
      "book_id": "PSA",
      "book_name": "Salmos",
      "chapter": 137,
      "verse_start": 3,
      "verse_end": 3,
      "text": [
        "Y",
        "los",
        "que",
        "allí",
        "nos",
        "habían",
        "llevado",
        "cautivos",
        "nos",
        "pedían",
        "que",
        "cantásemos,",
        "y",
        "los",
        "que",
        "nos",
        "habían",
        "desolado",
        "nos",
        "pedían",
        "alegría,",
        "<em>diciendo</em>:",
        "Cantadnos",
        "alguno",
        "de",
        "los",
        "cánticos",
        "de",
        "Sión."
      ],
      "info": ""
    },
    {
      "book_id": "PSA",
      "book_name": "Salmos",
      "chapter": 137,
      "verse_start": 4,
      "verse_end": 4,
      "text": [
        "¿Cómo",
        "cantaremos",
        "canción",
        "de",
        "Jehová",
        "en",
        "tierra",
        "de",
        "extraños?"
      ],
      "info": ""
    },
    {
      "book_id": "PSA",
      "book_name": "Salmos",
      "chapter": 137,
      "verse_start": 5,
      "verse_end": 5,
      "text": [
        "Si",
        "me",
        "olvidare",
        "de",
        "ti,",
        "oh",
        "Jerusalén,",
        "mi",
        "diestra",
        "olvide",
        "<em>su</em>",
        "<em>destreza</em>,"
      ],
      "info": ""
    },
    {
      "book_id": "PSA",
      "book_name": "Salmos",
      "chapter": 137,
      "verse_start": 6,
      "verse_end": 6,
      "text": [
        "mi",
        "lengua",
        "se",
        "pegue",
        "a",
        "mi",
        "paladar,",
        "si",
        "de",
        "ti",
        "no",
        "me",
        "acordare;",
        "si",
        "no",
        "enalteciere",
        "a",
        "Jerusalén",
        "como",
        "preferente",
        "asunto",
        "de",
        "mi",
        "alegría."
      ],
      "info": ""
    },
    {
      "book_id": "PSA",
      "book_name": "Salmos",
      "chapter": 137,
      "verse_start": 7,
      "verse_end": 7,
      "text": [
        "Acuérdate,",
        "oh",
        "Jehová,",
        "contra",
        "los",
        "hijos",
        "de",
        "Edom",
        "en",
        "el",
        "día",
        "de",
        "Jerusalén;",
        "los",
        "cuales",
        "decían:",
        "Arrasadla,",
        "arrasadla",
        "hasta",
        "los",
        "cimientos."
      ],
      "info": ""
    },
    {
      "book_id": "PSA",
      "book_name": "Salmos",
      "chapter": 137,
      "verse_start": 8,
      "verse_end": 8,
      "text": [
        "Hija",
        "de",
        "Babilonia,",
        "serás",
        "destruida,",
        "bienaventurado",
        "el",
        "que",
        "te",
        "diere",
        "el",
        "pago",
        "de",
        "lo",
        "que",
        "tú",
        "nos",
        "hiciste."
      ],
      "info": ""
    },
    {
      "book_id": "PSA",
      "book_name": "Salmos",
      "chapter": 137,
      "verse_start": 9,
      "verse_end": 9,
      "text": [
        "Bienaventurado",
        "el",
        "que",
        "tomare",
        "y",
        "estrellare",
        "tus",
        "niños",
        "contra",
        "las",
        "piedras."
      ],
      "info": ""
    }
  ]
