      "book_name": "Job",
      "chapter": 14,
      "verse_start": 7,
      "verse_end": 7,
      "text": [
        "Porque",
        "si",
        "el",
        "árbol",
        "fuere",
        "cortado,",
        "aún",
        "queda",
        "de",
        "él",
        "esperanza;",
        "retoñará",
        "aún,",
        "y",
        "sus",
        "renuevos",
        "no",
        "faltarán."
      ],
      "info": ""
    },
    {
      "book_id": "JOB",
      "book_name": "Job",
      "chapter": 14,
      "verse_start": 8,
      "verse_end": 8,
      "text": [
        "Si",
        "se",
        "envejeciere",
        "en",
        "la",
        "tierra",
        "su",
        "raíz,",
        "y",
        "su",
        "tronco",
        "fuere",
        "muerto",
        "en",
        "el",
        "polvo,"
      ],
      "info": ""
    },
    {
      "book_id": "JOB",
      "book_name": "Job",
      "chapter": 14,
      "verse_start": 9,
      "verse_end": 9,
      "text": [
