      "info": ""
    },
    {
      "book_id": "JOB",
      "book_name": "Job",
      "chapter": 30,
      "verse_start": 2,
      "verse_end": 2,
      "text": [
        "¿Y",
        "de",
        "qué",
        "me",
        "serviría",
        "la",
        "fuerza",
        "de",
        "sus",
        "manos,",
        "si",
        "el",
        "vigor",
        "de",
        "ellos",
        "ha",
        "perecido?"
      ],
      "info": ""
    },
    {
      "book_id": "JOB",
      "book_name": "Job",
      "chapter": 30,
      "verse_start": 3,
      "verse_end": 3,
      "text": [
        "Por",
        "causa",
        "de",
        "la",
        "pobreza",
        "y",
        "del",
        "hambre",
        "andaban",
        "solos;",
        "huían",
        "a",
        "la",
        "soledad,",
        "a",
        "lugar",
        "tenebroso,",
        "asolado",
        "y",
        "desierto."
      ],
      "info": ""
    },
    {
      "book_id": "JOB",
      "book_name": "Job",
      "chapter": 30,
      "verse_start": 4,
      "verse_end": 4,
      "text": [
        "Recogían",
