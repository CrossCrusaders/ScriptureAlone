      "info": ""
    },
    {
      "book_id": "JOB",
      "book_name": "Job",
      "chapter": 30,
      "verse_start": 5,
      "verse_end": 5,
      "text": [
        "Eran",
        "arrojados",
        "de",
        "entre",
        "<em>las</em>",
        "<em>gentes</em>,",
        "les",
        "gritaban",
        "como",
        "tras",
        "el",
        "ladrón."
      ],
      "info": ""
    },
    {
      "book_id": "JOB",
      "book_name": "Job",
      "chapter": 30,
      "verse_start": 6,
      "verse_end": 6,
      "text": [
