      "book_id": "1CH",
      "book_name": "1 Chronicles",
      "chapter": 1,
      "verse_start": 53,
      "verse_end": 53,
      "text": "Duke Kenaz, duke Teman, duke Mibzar,",
      "info": "",
      "language": "English"
    },
