      "book_id": "1CH",
      "book_name": "1 Chronicles",
      "chapter": 1,
      "verse_start": 21,
      "verse_end": 21,
      "text": "Hadoram also, and Uzal, and Diklah,",
      "info": "",
      "language": "English"
